@article{Paluck01032010,
author = {Paluck, Elizabeth Levy},
title = {The Promising Integration of Qualitative Methods and Field Experiments},
volume = {628},
number = {1},
pages = {59-71},
year = {2010},
doi = {10.1177/0002716209351510},
abstract ={Randomized field experiments should take a more central place in qualitative research. Although field experimentation is often considered a quantitative enterprise, this paper illustrates the compatibility of field experimentation with various types of qualitative measurement tools and research questions. Integrating qualitative and quantitative data within field experiments allows investigators to move past simple average treatment effects and explore mechanisms of the identified causal effect. A more novel proposal is to use field experimentation as the organizing methodological framework for archival, ethnographic, or interpretive work, and to use ethnographic methods as the primary source of measurement in “experimental ethnography.” Sustained research and theoretical specificity can address some of the seemingly incompatible features of qualitative and field experimental methods. For example, small sample sizes are acceptable as part of a research program, and some theories of historical patterns or rare events could be disaggregated into smaller cause-and-effect linkages to test with field experiments in theoretically relevant contexts.},
URL = {http://ann.sagepub.com/content/628/1/59.abstract},
eprint = {http://ann.sagepub.com/content/628/1/59.full.pdf+html},
journal = {The ANNALS of the American Academy of Political and Social Science}
}

